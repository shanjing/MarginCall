{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Run Success Rate",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0, "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.8, "color": "yellow" },
              { "value": 0.95, "color": "green" }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(margincall_runs_total{status=\"success\"}[5m])) / clamp_min(sum(rate(margincall_runs_total[5m])), 1)",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0, "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.6, "color": "yellow" },
              { "value": 0.8, "color": "green" }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(margincall_cache_ops_total{result=\"hit\"}[5m])) / clamp_min(sum(rate(margincall_cache_ops_total{operation=\"get\"}[5m])), 1)",
          "legendFormat": "Cache Hit Rate",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Active SSE Connections",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "targets": [
        {
          "expr": "margincall_active_sse_connections",
          "legendFormat": "SSE Clients",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Total Runs",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "targets": [
        {
          "expr": "sum(margincall_runs_total)",
          "legendFormat": "Total Runs",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Run Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(margincall_run_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(margincall_run_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(margincall_run_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
